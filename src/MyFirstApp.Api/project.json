{
    "version": "1.0.0-*",
    "dependencies": {
        "MyFirstApp.Infra.CrossCutting.IoC": {
            "target": "project"
        },
        "Microsoft.NETCore.App": {
            "version": "1.1.0",
            "type": "platform"
        },
        "Microsoft.AspNetCore": "1.0.3",
        "Microsoft.AspNetCore.Mvc": "1.0.2",
        "Microsoft.Extensions.Logging.Debug": "1.0.1"
    },
    "frameworks": {
        "netcoreapp1.0": {
            "imports": []
        }
    },
    "buildOptions": {
        "warningsAsErrors": true,
        "emitEntryPoint": true,
        "copyToOutput": [
            "./appsettings.json"
        ]
    },
    "tools": {
        "Microsoft.EntityFrameworkCore.Tools.DotNet": "1.1.0-preview4-final"
    },
    "Logging": {
        "IncludeScopes": false,
        "LogLevel": {
            "Default": "Debug",
            "System": "Information",
            "Microsoft": "Information"
        }
    }
}