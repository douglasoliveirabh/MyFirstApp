{
    "version": "1.0.0-*",
    "testRunner": "xunit",
    "dependencies": {
        "MyFirstApp.Domain": {
            "target": "project"
        },
        "MyFirstApp.Infra.Data": {
            "target": "project"
        },
        "Microsoft.EntityFrameworkCore.InMemory": "1.0.0",
        "dotnet-test-xunit": "2.2.0-preview2-build1029",
        "xunit": "2.2.0-beta2-build3300"
    },
    "frameworks": {
        "netcoreapp1.1": {
            "dependencies": {
                "Microsoft.NETCore.App": {
                    "type": "platform",
                    "version": "1.1.0"
                }
            }
        }
    },
    "buildOptions": {
        "copyToOutput": {
            "include": [
                "xunit.runner.json"
            ]
        }
    },
    "tooling": {
        "defaultNamespace": "MyFirstApp.Test.Unit"
    }
}